{
  "name": "pansiep",
  "version": "0.0.0",
  "description": "Screeps AI",
  "main": "nm",
  "scripts": {
    "build": "rollup -c",
    "watch-build": "rollup -cw",
    "lint": "eslint **/*",
    "lint-fix": "eslint **/* --fix",
    "push-main": "rollup -c --environment DEST:main",
    "push-s+": "rollup -c --environment DEST:s+",
    "push-pserver": "rollup -c --environment DEST:pserver",
    "push-pserver2": "rollup -c --environment DEST:pserver2",
    "push-sim": "rollup -c --environment DEST:sim",
    "test": "jest",
    "test-server": "npm run build && node server/index.js 300",
    "test-clearCache": "jest --clearCache",
    "test-verbose": "jest --verbose",
    "test-watch": "jest --watch --verbose",
    "test-cov": "jest --coverage",
    "test-cov-runInBand": "jest --ci --coverage --runInBand",
    "test-only": "jest --watch --coverage",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-pserver2": "rollup -cw --environment DEST:pserver2",
    "watch-sim": "rollup -cw --environment DEST:sim",
    "upload-coverage": "npm run test-cov-runInBand && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pieterbrandsen/Panda-AI.git"
  },
  "author": "Pieter Brandsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pieterbrandsen/Pansiep/issues"
  },
  "homepage": "https://github.com/pieterbrandsen/Pansiep#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.27",
    "@types/screeps": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "codecov": "^3.8.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-screeps": "^2.1.0",
    "jest": "^27.1.0",
    "jsdoc": "^3.6.6",
    "prettier": "2.2.1",
    "q": "^1.5.1",
    "rimraf": "3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "screeps": "4.2.3",
    "screeps-api": "1.13.0",
    "screepsmod-admin-utils": "1.24.0",
    "screepsmod-auth": "2.6.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "screeps-jest": "^2.0.0",
    "source-map": "~0.6.1",
    "ts-mixer": "^6.0.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
